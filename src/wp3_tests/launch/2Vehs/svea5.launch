    <!-- ABConnect Connectors -->
    <node name="ros_abconnect__add_remote_topic__1ee168d2a7394c55b055a89eb33c2077" pkg="ros_abconnect" type="add_remote_topic.py" output="screen">
        <rosparam>
            role: subscriber
            client: debby
            id: 1
            topic: start
            type: std_msgs/Empty
        </rosparam>
    </node>
    <node name="ros_abconnect__add_remote_topic__cf067670d79f41f182a9288375e141f0" pkg="ros_abconnect" type="add_remote_topic.py" output="screen">
        <rosparam>
            role: subscriber
            client: svea2
            id: 3
            topic: incoming
            type: wp3_tests/Packet
        </rosparam>
    </node>
    <node name="ros_abconnect__add_remote_topic__1c1ba63e73384405ada7f1112335a439" pkg="ros_abconnect" type="add_remote_topic.py" output="screen">
        <rosparam>
            role: publisher
            client: svea2
            id: 4
            topic: outgoing
            type: wp3_tests/Packet
        </rosparam>
    </node>
    <node name="ros_abconnect__add_remote_topic__6c4380ab92f3469ba377fd1ec45eeec0" pkg="ros_abconnect" type="add_remote_topic.py" output="screen">
        <rosparam>
            role: publisher
            client: svea5
            id: 5
