            type: wp3_tests/Packet
        </rosparam>
    </node>
    <node name="ros_abconnect__add_remote_topic__0e1052b08a6c4797bba07b6af5bb77b6" pkg="ros_abconnect" type="add_remote_topic.py" output="screen">
        <rosparam>
            role: subscriber
            client: svea5
            id: 5
            topic: incoming
            type: wp3_tests/Packet
        </rosparam>
    </node>
