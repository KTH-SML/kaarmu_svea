<?xml version="1.0"?>
<launch>
    <!-- Meta Data -->
    <rosparam>
        name: debby
        master: debby
        clients: [debby, svea2, svea5]
    </rosparam>

    <!-- ABConnect Wrapper -->
    <include file="$(find ros_abconnect)/launch/abconnect.launch">
        <arg name="host" value="wss://intelligent-cluster-manager.herokuapp.com/signal"/>
        <arg name="name" value="debby"/>
        <arg name="category" value="device"/>
        <arg name="start_delay" value="5"/>
